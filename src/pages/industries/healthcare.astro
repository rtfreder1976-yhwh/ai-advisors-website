      "serviceType": "Healthcare AI Automation",
      "audience": {
        "@type": "Audience",
        "audienceType": "Medical Practices and Healthcare Providers"
      }
    },
    {
      "@type": "WebPage",
      "@id": "https://singingriverai.com/industries/healthcare/#webpage",
