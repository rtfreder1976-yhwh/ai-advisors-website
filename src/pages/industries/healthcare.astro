      "itemListElement": [
        { "@type": "ListItem", "position": 1, "name": "Home", "item": "https://singingriverai.com" },
        { "@type": "ListItem", "position": 2, "name": "Healthcare AI", "item": "https://singingriverai.com/industries/healthcare" }
      ]
    },
    {
      "@type": "FAQPage",
      "@id": "https://singingriverai.com/industries/healthcare/#faq",
