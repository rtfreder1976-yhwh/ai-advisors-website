const title = "HIPAA-Compliant AI for Medical Practices in Huntsville & North Alabama | Healthcare AI Automation | Singing River AI";
const description = "HIPAA-compliant AI automation for medical practices, clinics, and healthcare providers in Huntsville, Florence, and North Alabama. Patient scheduling, insurance verification, and staff optimization without compromising compliance.";
const canonicalURL = new URL(Astro.url.pathname, Astro.site);
const ogImageURL = new URL('/og-image.png', Astro.site);

const solutions = [
  {
    title: "Patient Scheduling Automation",
    description: "AI handles appointment requests 24/7, reducing no-shows with automated reminders and making rebooking seamless. Patients can schedule, reschedule, or cancel without calling during office hours.",
    stat: "30%",
    statLabel: "reduction in no-shows"
  },
  {
    title: "Insurance Verification",
    description: "Automate eligibility checks before appointments. AI verifies coverage, identifies co-pays, and flags issues before patients arrive—reducing claim denials and front-desk headaches.",
    stat: "15+",
    statLabel: "hours saved weekly"
  },
  {
    title: "Patient Communication",
    description: "Automated appointment reminders, follow-up care instructions, and prescription refill notifications. Keep patients engaged and informed without adding staff workload.",
    stat: "40%",
    statLabel: "fewer phone calls"
  },
  {
    title: "Staff Scheduling & Optimization",
    description: "AI analyzes patient flow patterns to optimize staff schedules, reduce overtime, and ensure coverage during peak hours. Better utilization without burnout.",
    stat: "20%",
    statLabel: "improved staff efficiency"
  }
];

const useCases = [
  {
    title: "Front Desk Automation",
    items: [
      "24/7 appointment scheduling",
      "Automated appointment reminders",
